<section class="page legislator-page">
  <div class="row">
    <h1 class="title">
      {{model.title}}. {{model.first_name}} {{model.last_name}}  
    </h1>
     <h2 class="office">
      {{model.state_name}} {{model.formattedChamber}}, {{model.formattedParty}} 
    </h2>
  </div>
  <section class="main-content">
    <div class="row">
      <div class="colum">
        <h5>Top Contributors</h5>
        <ul>
          {{#each model.contributors}}
            <li><strong>{{name}}</strong><em>{{format-dollars total_amount}}</em></li>
          {{/each}}
        </ul>
      </div>
      <div class="colum">
        <h5>Contact</h5>
      </div>
    </div>
  </section>
</section>